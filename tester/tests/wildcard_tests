echo * | sort
export ARG="*"
echo $ARG | sort
ls *
ls 'test'*
ls "test"*
touch "*1" ; echo "*"* | sort ; rm "*1"
touch "   " ; ls * | grep "   " ; rm "   "
touch "a" "aa" ; echo *a | sort ; rm "a" "aa"
